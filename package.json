{
  "name": "vscode-hzbcode-plugin",
  "publisher": "jialtang",
  "displayName": "vscode-hzbcode-plugin",
  "icon": "images/ai-logo.jpg",
  "description": "vscode-hzbcode-plugin",
  "version": "0.2.0",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/jialtang/vscode-chatgpt-plugin"
  },
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Testing",
    "Data Science",
    "Formatters",
    "Programming Languages",
    "Linters"
  ],
  "keywords": [
    "hzbcode",
    "vscode-hzbcode-plugin",
    "vscode-hzbcode",
    "hzbcode-plugin",
    "hzbcode-vscode",
    "hzbcode-vscode-plugin"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "workbench.view.extension.vscode-hzbcode-view-container",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c"
      }
    ],
    "submenus": [
      {
        "id": "vscode-hzbcode.subMenu",
        "label": "hzbcode-plugin"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "id": "hzbcode-hzbcode.menu",
          "group": "hzbcode",
          "submenu": "vscode-hzbcode.subMenu"
        }
      ],
      "vscode-hzbcode.subMenu": [
        {
          "command": "vscode-hzbcode.findBugs",
          "group": "hzbcode@3",
          "when": "editorHasSelection && findBugs-enabled"
        },
        {
          "command": "vscode-hzbcode.explain",
          "group": "hzbcode@5",
          "when": "editorHasSelection && explain-enabled"
        },
        {
          "command": "vscode-hzbcode.addComments",
          "group": "hzbcode@6",
          "when": "editorHasSelection && addComments-enabled"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-hzbcode.freeText",
        "title": "%vscode-hzbcode.freeText.title%"
      },
      {
        "command": "vscode-hzbcode.clearSession",
        "title": "%vscode-hzbcode.clearSession.title%"
      },
      {
        "command": "vscode-hzbcode.findBugs",
        "title": "%vscode-hzbcode.findBugs.title%",
        "enablement": "editorHasSelection"
      },
      {
        "command": "vscode-hzbcode.explain",
        "title": "%vscode-hzbcode.explain.title%",
        "enablement": "editorHasSelection"
      },
      {
        "command": "vscode-hzbcode.addComments",
        "title": "%vscode-hzbcode.addComments.title%",
        "enablement": "editorHasSelection"
      },
      {
        "command": "vscode-hzbcode.clearConversation",
        "title": "%vscode-hzbcode.clearConversation.title%"
      },
      {
        "command": "vscode-hzbcode.exportConversation",
        "title": "%vscode-hzbcode.exportConversation.title%"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscode-hzbcode-view-container",
          "title": "hzbcode-plugin",
          "icon": "images/openai-logo.svg"
        }
      ]
    },
    "views": {
      "vscode-hzbcode-view-container": [
        {
          "type": "webview",
          "id": "vscode-hzbcode-plugin.view",
          "name": "%vscode-hzbcode-plugin-view-container.name%"
        }
      ]
    },
    "configuration": {
      "title": "%hzbcode-config.title%",
      "type": "object",
      "properties": {
        "hzbcode.response.subscribeToResponse": {
          "type": "boolean",
          "default": false,
          "description": "%hzbcode.response.subscribeToResponse.description%",
          "order": 10
        },
        "hzbcode.response.autoScroll": {
          "type": "boolean",
          "default": true,
          "description": "%hzbcode.response.autoScroll.description%",
          "order": 11
        },
        "hzbcode.promptPrefix.findBugs-enabled": {
          "type": "boolean",
          "default": true,
          "description": "%hzbcode.promptPrefix.findBugs-enabled.description%",
          "order": 15
        },
        "hzbcode.promptPrefix.explain-enabled": {
          "type": "boolean",
          "default": true,
          "description": "%hzbcode.promptPrefix.explain-enabled.description%",
          "order": 19
        },
        "hzbcode.promptPrefix.addComments-enabled": {
          "type": "boolean",
          "default": true,
          "description": "%hzbcode.promptPrefix.addComments-enabled.description%",
          "order": 21
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "rimraf out && npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "build": "npm run -S esbuild-base -- --sourcemap",
    "watch": "npm run -S esbuild-base -- --sourcemap --watch",
    "fmt": "prettier --write \"src/**/*.ts\"&& npm run test -- --fix",
    "test": "eslint src --ext ts && tsc --noEmit"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/uuid": "^9.0.0",
    "@types/vscode": "^1.68.0",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "conventional-changelog": "^5.1.0",
    "esbuild": "^0.15.18",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "husky": "^1.3.1",
    "mocha": "^10.1.0",
    "prettier": "^2.8.7",
    "pretty-quick": "^1.8.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "vcm-cli": "^1.0.5"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "delay": "^5.0.0",
    "eventsource-parser": "^0.1.0",
    "gpt-tokenizer": "^2.1.1",
    "gpt3-tokenizer": "^1.1.5",
    "isomorphic-fetch": "^3.0.0",
    "keyv": "^4.5.2",
    "p-timeout": "^6.1.1",
    "puppeteer": "^19.4.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "puppeteer-extra-plugin-user-data-dir": "^2.4.0",
    "puppeteer-extra-plugin-user-preferences": "^2.4.0",
    "quick-lru": "^6.1.1",
    "remark": "^14.0.2",
    "strip-markdown": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "vcm"
    }
  },
  "resolutions": {
    "clone-deep": "^4.0.1"
  },
  "volta": {
    "node": "16.20.0"
  }
}